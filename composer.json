{
  "name": "barthy-koeln/cached-prezent-translation",
  "description": "Provides a trait to be used with prezent/translatable-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Barthy",
      "email": "post@barthy.koeln"
    }
  ],
  "keywords": [
    "translatable",
    "cached"
  ],
  "homepage": "https://github.com/BarthyB/cached-prezent-translation",
  "type": "library",
  "require": {
    "php": "^7.4|^8.1",
    "prezent/doctrine-translatable-bundle": "^1.2"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16",
    "phpmd/phpmd": "^2.9",
    "squizlabs/php_codesniffer": "^3.5",
    "brainmaestro/composer-git-hooks": "^2.8",
    "phpunit/phpunit": "^9.4",
    "barthy-koeln/beautify-specify": "^1.0",
    "codeception/assert-throws": "^1.1"
  },
  "extra": {
    "hooks": {
      "pre-commit": [
        "bin/pre-commit"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "BarthyKoeln\\CachedPrezentTranslation\\": "lib/Barthy/CachedPrezentTranslation/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BarthyKoeln\\CachedPrezentTranslation\\Tests\\": "tests/"
    }
  }
}
